{
"flow_mod":
    {
    "_name": "vpws_add_vlan_flow",
    "_description": "VPWS termination: add VLAN flow",
    "table": "TABLE_VLAN",
    "cmd": "add",
    "mask": "0",
    "port": "any",
    "group": "any",
    "match":
